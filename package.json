{
  "name": "weather-app-rezolve-ai",
  "version": "1.0.0",
  "description": "Nodejs app that takes a request of a place name and responds with the weather in that place and stores the request details in DB. Display all requests received for the given time period",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:prod": "NODE_ENV=production nodemon server.js",
    "debug": "ndb server.js",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js"
  },
  "author": "abinesh_eswaran",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.0.1",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.4.1"
  },
  "engines": {
    "node": "^10"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.1.0",
    "express-rate-limit": "^5.5.0",
    "helmet": "^4.6.0",
    "hpp": "^0.2.3",
    "mongoose": "^5.12.7",
    "morgan": "^1.10.0",
    "node-fetch": "^2.0.0",
    "pug": "^3.0.2",
    "request": "^2.88.2",
    "xss-clean": "^0.1.1"
  }
}
